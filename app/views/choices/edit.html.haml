%h1 Edit Choice
%h4= @question.body << " > " <<  @choice.body
%table
  %tr
    %th
      Outcome Title
    %th
      Strength
  - @choice.weights.each do |weight|
    %tr
      %td
        = weight.outcome.name
      %td
        = weight.strength


=link_to "Add Weight", new_choice_weight_path(@choice), class: 'button'
